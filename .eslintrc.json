{
    "extends": ["eslint:recommended", "google"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-console": ["error", {
            "allow": ["warn", "error"]
        }]
    }
}